    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - FraxControl</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        
    </head>

 @section styles{
    <style>
        * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: "Poppins", sans-serif;
    }

    .navbar{
        background-color: #fff;
        height: 80px;
        margin: 20px;
        border-radius: 16px;
        padding: 0.5rem;
    }

    .navbar-brand{
        font-weight: 500;
        color: rgba(91, 132, 154);
        font-size: 24px;
        transition: 0.3s color;
    }

    .navbar-toggler{
        border: none;
        font-size: 1.25rem;
    }
    .navbar-toggler:focus, .btn-close:focus{
        box-shadow: none;
        outline: none;
    }
    .nav-link{
        color: #666777;
        font-weight: 500;
        position: relative;
    }
    .nav-link:hover, .nav-link.active{
        color: #000;
    }
    </style>
 }



    <head>
    <nav class="navbar navbar-expand-lg fixed-top">
  <div class="container-fluid">
    <a class="navbar-brand me-auto"  asp-controller="Home" asp-action="Index">FraxControl</a>
    <button class="navbar-toggler text-dark" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar" aria-controls="offcanvasNavbar" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
      <div class="offcanvas-header">
        <h5 class="offcanvas-title" id="offcanvasNavbarLabel">FraxControl</h5>
        <button type="button" class="btn-close text-dark" data-bs-dismiss="offcanvas" aria-label="Close"></button>
      </div>
      <div class="offcanvas-body">
        <ul class="navbar-nav justify-content-center flex-grow-1 pe-3">
          <li class="nav-item">
            <a class="nav-link text-dark mx-lg-2" aria-current="page" asp-controller="Home" asp-action="Index">Home</a>
          </li>
          <li class="nav-item">
                <a class="nav-link text-dark mx-lg-2" asp-area="" asp-controller="SocialArea" asp-action="SocialAreaList">Area Social</a>
          </li>
          <li class="nav-item">
                <a class="nav-link text-dark mx-lg-2" asp-area="" asp-controller="Vehiculos" asp-action="VehiculosList">Vehiculos</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-dark mx-lg-2" asp-area="" asp-controller="Visitas" asp-action="VisitasList">Registrar Visitas</a>
            </li>


        </ul>
      </div>
    </div>
  </div>
</nav>



